{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["deno.window", "dom", "dom.iterable", "esnext"],
    "strict": true,
    "module": "esnext",
    "target": "esnext"
  },
  "imports": {
    "react": "https://esm.sh/react@19.2.0",
    "react-dom": "https://esm.sh/react-dom@19.2.0",
    "react-dom/server": "https://esm.sh/react-dom@19.2.0/server"
  },
  "tasks": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "start": "deno run --allow-net --allow-read --allow-env server.ts"
  },
  "fmt": {
    "include": ["src/", "*.ts", "*.tsx"],
    "exclude": ["src/routeTree.gen.ts"]
  },
  "lint": {
    "include": ["src/", "*.ts", "*.tsx"],
    "exclude": ["src/routeTree.gen.ts", "**/routeTree.gen.ts"]
  }
}
